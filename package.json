{
  "name": "tree-sitter-eu4-mod",
  "version": "0.0.1",
  "description": "Global grammar tree-sitter for modding files in Europa Universalis IV.",
  "main": "index.js",
  "keywords": [
    "parser",
    "eu4",
    "europa",
    "universalis",
    "IV",
    "paradox",
    "mod"
  ],
  "author": "Richard ANDRIAN (Coyote-31)",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.16.4"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "test": "tree-sitter test",
    "test-windows": "tree-sitter test",
    "build-test": "tree-sitter generate && node-gyp build && tree-sitter test",
    "watch-test": "npm-watch test",
    "watch-grammar": "npm-watch build-test"
  },
  "tree-sitter": [
    {
      "scope": "source.eu4-mod",
      "file-types": [
        "mod"
      ],
      "injection-regex": "^eu4-mod$"
    }
  ]
}
