{
  "name": "tree-sitter-eu4-mod",
  "version": "0.0.11",
  "description": "Global grammar tree-sitter for modding files in Europa Universalis IV.",
  "main": "index.js",
  "keywords": [
    "parser",
    "eu4",
    "europa",
    "universalis",
    "IV",
    "paradox",
    "mod"
  ],
  "author": "Richard ANDRIAN - Coyote-31 <coyotepouil@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Coyote-31/tree-sitter-eu4-mod/issues"
  },
  "homepage": "https://github.com/Coyote-31/tree-sitter-eu4-mod/#readme",
  "dependencies": {
    "nan": "^2.14.0"
  },
  "devDependencies": {
    "prebuild": "^10.0.0",
    "prebuild-install": "^5.3.3",
    "tree-sitter-cli": "^0.16.4"
  },
  "scripts": {
    "install": "prebuild-install -t 4.2.7 -r electron --verbose || node-gyp rebuild",
    "build": "tree-sitter generate && node-gyp build -target=v10.11.0 --arch=x64",
    "prebuild": "prebuild -t 4.2.7 -r electron",
    "rebuild": "node-gyp rebuild -target=v10.11.0 --arch=x64",
    "build-test": "tree-sitter generate && tree-sitter test && node-gyp build"
  },
  "repository": {
    "type": "git",
    "url": "github:Coyote-31/tree-sitter-eu4mod"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  }
}
