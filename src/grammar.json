{
  "name": "eu4mod",
  "rules": {
    "file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dot_mod"
        }
      ]
    },
    "dot_mod": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_carriage_return"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "mod_mono_line"
            },
            {
              "type": "SYMBOL",
              "name": "mod_multi_line"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "REPEAT1",
                "content": {
                  "type": "SYMBOL",
                  "name": "_carriage_return"
                }
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "mod_mono_line"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "mod_multi_line"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_carriage_return"
          }
        }
      ]
    },
    "mod_mono_line": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "warning_space_tab"
            }
          },
          {
            "type": "SYMBOL",
            "name": "mod_var_name"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "warning_space_tab"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "REPEAT",
                    "content": {
                      "type": "SYMBOL",
                      "name": "warning_space_tab"
                    }
                  },
                  {
                    "type": "SYMBOL",
                    "name": "mod_equal"
                  },
                  {
                    "type": "REPEAT",
                    "content": {
                      "type": "SYMBOL",
                      "name": "warning_space_tab"
                    }
                  },
                  {
                    "type": "SYMBOL",
                    "name": "mod_var_value"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "mod_multi_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "warning_space_tab"
          }
        },
        {
          "type": "SYMBOL",
          "name": "mod_var_name_multi"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "warning_space_tab"
                  }
                },
                {
                  "type": "SYMBOL",
                  "name": "mod_equal"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "warning_space_tab"
                  }
                },
                {
                  "type": "STRING",
                  "value": "{"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "mod_multi_line_content"
                  }
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "warning_space_tab"
                  }
                },
                {
                  "type": "STRING",
                  "value": "}"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "mod_multi_line_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "warning_space_tab"
              }
            },
            {
              "type": "SYMBOL",
              "name": "mod_var_value"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_carriage_return"
                  },
                  {
                    "type": "PATTERN",
                    "value": "\\t| {2}"
                  },
                  {
                    "type": "REPEAT",
                    "content": {
                      "type": "SYMBOL",
                      "name": "warning_space_tab"
                    }
                  },
                  {
                    "type": "SYMBOL",
                    "name": "mod_var_value"
                  }
                ]
              }
            },
            {
              "type": "SYMBOL",
              "name": "_carriage_return"
            }
          ]
        }
      ]
    },
    "mod_var_name": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "name"
          },
          {
            "type": "STRING",
            "value": "supported_version"
          },
          {
            "type": "STRING",
            "value": "version"
          },
          {
            "type": "STRING",
            "value": "path"
          },
          {
            "type": "STRING",
            "value": "archive"
          },
          {
            "type": "STRING",
            "value": "picture"
          },
          {
            "type": "STRING",
            "value": "remote_file_id"
          }
        ]
      }
    },
    "mod_var_name_multi": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "tags"
          },
          {
            "type": "STRING",
            "value": "dependencies"
          }
        ]
      }
    },
    "mod_equal": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "STRING",
        "value": "="
      }
    },
    "mod_var_value": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\\"[^\\\"\\n]*\\\""
      }
    },
    "_carriage_return": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "warning_space_tab": {
      "type": "PATTERN",
      "value": "\\t| ]"
    },
    "debug": {
      "type": "PATTERN",
      "value": ".+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

